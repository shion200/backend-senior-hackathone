{ 
    "regions" : [
        "hnd1"
    ],
    "builds": [
        { 
            "src": "main.py", 
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "static"
            }
        }
    ],
    "routes": [
        { 
            "src": "//setCalorieOfGoal", 
            "dest": "main.py" 
        },
        {
            "src" : "/getCalorieRanking",
            "dest" : "main.py"
        },
        {
            "src" : "/setCalorieOfDay",
            "dest" : "main.py"
        },
        {
            "src" : "/caloKUSA",
            "dest" : "main.py"
        },
        {
            "src" : "/signin",
            "dest" : "main.py"
        },
        {
            "src" : "/signup",
            "dest" : "main.py"
        }
    ]
   }